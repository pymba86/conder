{
  "app": {
    "name": "Condersp Gateway",
    "displayBanner": true
  },
  "logger": {
    "level": "Information",
    "console": {
      "enabled": true
    }
  },
  "http": {
    "retires": 2,
    "interval": 2.0,
    "exponential": true
  },
  "loadBalancer": {
    "enabled": false,
    "url": "localhost"
  },
  "modules": {
    "orders" : {
      "routes" : [
        {
          "upstream": "/",
          "method": "GET",
          "use": "return_value",
          "returnValue": "Welcome to orders api"
        }
      ]
    }
  }
}
