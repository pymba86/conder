{
  "app": {
    "name": "Condersp Orders Service",
    "displayBanner": true
  },
  "logger": {
    "level": "Information",
    "console": {
      "enabled": true
    }
  },
  "swagger": {
    "enabled": true,
    "name": "v2",
    "title": "Orders Service",
    "version": "v1",
    "routePrefix": "docs"
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "orders-service",
    "address": "localhost",
    "port": "5000",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 3,
    "removeAfterInterval": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "orders-service"
  }
}
